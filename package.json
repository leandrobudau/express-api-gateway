{
  "name": "express-api-gateway",
  "description": "Simples api gateway usando expressjs",
  "version": "0.4.1",
  "author": {
    "name": "Leandro Budau",
    "email": "leandrobudau@gmail.com",
    "url": "https://leandrobudau.com"
  },
  "scripts": {
    "start": "nodemon index.js",
    "build": "tsc",
    "prepare": "husky install",
    "lint": "eslint -f unix \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier --write .",
    "docs": "typedoc"
  },
  "main": "dist/index.js",
  "lint-staged": {
    "**/*.ts": "prettier --write --ignore-unknown"
  },
  "peerDependencies": {
    "express": ">=4"
  },
  "dependencies": {
    "express": ">=4",
    "express-http-proxy": "^1.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-http-proxy": "^1.6.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.3"
  },
  "files": [
    "./dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/leandrobudau/express-api-gateway"
  }
}
